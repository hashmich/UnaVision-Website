<IfModule mod_rewrite.c>
    RewriteBase /
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    #RewriteCond %{HTTPS} off
    #RewriteRule ^/?(.*) https://%{SERVER_NAME}/index.php?q=$1 [R=301,L]
    RewriteRule ^(.*)$ index.php?q=$1 [L]
</IfModule>
